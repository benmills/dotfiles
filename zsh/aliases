alias bt-ws-start='VBoxManage startvm bt-workstation --type headless && open /Applications/CoRD.app'
alias bt-ws-stop='VBoxManage controlvm bt-workstation poweroff'
alias ghost-start='VBoxManage startvm ghost --type headless && open /Applications/CoRD.app'
alias ghost-stop='VBoxManage controlvm ghost poweroff'
alias run-ant='ant -logger org.apache.tools.ant.listener.AnsiColorLogger'
alias vimnote='vim -c Note ~/Dropbox/notes'
