ncrease volume
"amixer set Master playback 10+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

# Decrease volume
"amixer set Master playback 10-"
    m:0x0 + c:122
    XF86AudioLowerVolume

# Toggle mute
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute

"~/.bin/rdio-play-pause"
    m:0x0 + c:172
    XF86AudioPlay

"~/.bin/rdio-prev"
    m:0x0 + c:173
    XF86AudioPrev

"~/.bin/rdio-next"
    m:0x0 + c:171
    XF86AudioNext

