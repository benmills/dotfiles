[color]
  ui = auto
[alias]
  ci = commit
  co = checkout
  st = status
  ch = cherry -v origin
  br = branch
  addall = add -A .
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short

  # show difference between the HEAD and the index
  staged = diff --cached

  # show difference between working tree and the index
  unstaged = diff
